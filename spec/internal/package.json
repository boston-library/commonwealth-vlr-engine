{
  "name": "internal",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@popperjs/core": "2.11.8",
    "autoprefixer": "^10.4.20",
    "blacklight-frontend": "8.11.0",
    "blacklight-gallery": "^4.9.0",
    "bootstrap": "5.3.3",
    "bootstrap-icons": "1.11.3",
    "commonwealth-vlr-engine": "link:./../../../commonwealth-vlr-engine",
    "masonry-layout": "v4",
    "nodemon": "^3.1.9",
    "openseadragon-rails": "^1.0.17",
    "postcss": "^8.5.3",
    "postcss-cli": "^11.0.0",
    "sass": "^1.85.1"
  },
  "scripts": {
    "build:css:compile": "sass ./app/assets/stylesheets/application.bootstrap.scss ./app/assets/builds/application.css --no-source-map --load-path=node_modules",
    "build:css:prefix": "postcss ./app/assets/builds/application.css --use=autoprefixer --output=./app/assets/builds/application.css",
    "build:css": "yarn build:css:compile && yarn build:css:prefix",
    "watch:css": "nodemon --watch ./app/assets/stylesheets/ --watch ./../../../commonwealth-vlr-engine/app/assets/stylesheets/ --ext scss --exec \"yarn build:css\""
  },
  "browserslist": [
    "defaults"
  ]
}
